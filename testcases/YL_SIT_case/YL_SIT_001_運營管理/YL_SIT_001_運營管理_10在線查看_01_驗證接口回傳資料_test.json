{
  "config": {
    "name": "001_運營管理_在線查看_01_驗證接口回傳資料filter=True",
    "base_url": "http://192.168.33.5:9100",
    "variables": {
      "_getinfo": "${get_backend_info(YL)}",
      "connect_sid": "${get_connect_sid(YL)}",
      "headers_cookie": "${get_headers_cookie(YL)}"
    }
  },
  "teststeps": [
    {
      "name": "/onlineplayer",
      "request": {
        "method": "GET",
        "url": "/onlineplayer",
        "headers": {
          "If-None-Match": "W/\"c58-IZ7i/2l4Yk+rXwid3DcEVQuX3l8\"",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 304
        }
      ]
    },
    {
      "name": "/getCurPageGrant",
      "request": {
        "method": "GET",
        "url": "/getCurPageGrant",
        "params": {
          "powerid": "195"
        },
        "headers": {
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain"
        }
      ]
    },
    {
      "name": "/onlineplayer/get24HoursOnline",
      "request": {
        "method": "GET",
        "url": "/onlineplayer/get24HoursOnline",
        "params": {
          "interval": "1"
        },
        "headers": {
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        }
      ]
    }
  ]
}