  "teststeps": [
    {
      "name": "/statisAgentInfo",
      "request": {
        "method": "GET",
        "url": "/statisAgentInfo",
        "headers": {
          "If-None-Match": "W/\"2279-XiN1jgOvuFGji1/PZzDp9qQfCLA\"",
          "Proxy-Connection": "keep-alive",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 304
        }
      ]
    },
