{
  "config": {
    "name": "001_運營管理_IP白名單_開啟子板塊filter=True",
    "base_url": "http://192.168.33.5:9100",
    "variables": {
      "_getinfo": "${get_backend_info(YL)}",
      "connect_sid": "${get_connect_sid(YL)}",
      "headers_cookie": "${get_headers_cookie(YL)}"
    }
  },
  "teststeps": [
    {
      "name": "/iPwhitelis/GetList",
      "request": {
        "method": "GET",
        "url": "/iPwhitelis/GetList",
        "params": {
          "id": "",
          "limit": "10",
          "offset": "0",
          "total": "0"
        },
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "extract": {
        "exID": "body.rows[0].ID"
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        }
      ]
    },
    {
      "name": "/iPwhitelis/GetList",
      "request": {
        "method": "GET",
        "url": "/iPwhitelis/GetList",
        "params": {
          "id": "",
          "limit": "10",
          "offset": "0",
          "total": "0"
        },
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        },
        {
          "check": "$exID",
          "assert": "eq",
          "expect": 91,
          "msg": "check id"
        }
      ]
    }
  ]
}