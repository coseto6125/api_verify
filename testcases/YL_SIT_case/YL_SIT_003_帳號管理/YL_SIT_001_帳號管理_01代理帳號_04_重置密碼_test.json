{
  "config": {
    "name": "001_帳號管理_代理帳號_04_重置密碼filter=True",
    "base_url": "http://192.168.33.5:9100",
    "variables": {
      "_getinfo": "${get_backend_info(YL)}",
      "connect_sid": "${get_connect_sid(YL)}",
      "headers_cookie": "${get_headers_cookie(YL)}"
    }
  },
  "teststeps": [
    {
      "name": "/proxyaccount/GetList",
      "request": {
        "method": "GET",
        "url": "/proxyaccount/GetList",
        "params": {
          "Accounts": "",
          "BeginDate": "2023-01-23",
          "ChannelID": "161",
          "EndDate": "2023-02-01",
          "NickName": "",
          "limit": "50",
          "offset": "0",
          "proxyUID": "",
          "proxyUIDS": ""
        },
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain"
        }
      ]
    },
    {
      "name": "/proxyaccount/resetproxyPassword",
      "request": {
        "method": "POST",
        "url": "/proxyaccount/resetproxyPassword",
        "headers": {
          "Content-Type": "application/x-www-form-urlencoded; charset=UTF-8",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        },
        "body": {
          "id": "161"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain"
        }
      ]
    },
    {
      "name": "/proxyaccount/GetList",
      "request": {
        "method": "GET",
        "url": "/proxyaccount/GetList",
        "params": {
          "Accounts": "",
          "BeginDate": "2023-01-23",
          "ChannelID": "",
          "EndDate": "2023-02-01",
          "NickName": "",
          "limit": "50",
          "offset": "0",
          "proxyUID": "",
          "proxyUIDS": ""
        },
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain"
        }
      ]
    }
  ]
}