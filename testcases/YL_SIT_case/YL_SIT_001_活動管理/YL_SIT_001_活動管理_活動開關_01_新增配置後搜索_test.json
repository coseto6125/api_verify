{
  "config": {
    "name": "001_活動管理_活動開關_01_新增配置後搜索filter=True",
    "base_url": "http://192.168.33.5:9100",
    "variables": {
      "_getinfo": "${get_backend_info(YL)}",
      "connect_sid": "${get_connect_sid(YL)}",
      "headers_cookie": "${get_headers_cookie(YL)}"
    }
  },
  "teststeps": [
    {
      "name": "/ActivitySwitch/addConfig",
      "request": {
        "method": "POST",
        "url": "/ActivitySwitch/addConfig",
        "headers": {
          "Content-Type": "application/x-www-form-urlencoded; charset=UTF-8",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        },
        "body": {
          "ChannelID": "149",
          "_csrf": "jWqSa1fA-JyNo9spr9iva_BT_UAWRTiSuAnc",
          "activityID": "11012",
          "childID": "149"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        }
      ]
    },
    {
      "name": "/ActivitySwitch/getList",
      "request": {
        "method": "GET",
        "url": "/ActivitySwitch/getList",
        "params": {
          "ChannelID": "",
          "beginDate": "2023-02-06 17:44",
          "endDate": "2023-02-06 17:44",
          "limit": "20",
          "offset": "0"
        },
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        }
      ]
    }
  ]
}