{
  "config": {
    "name": "001_財務管理_體育交收報表_02_生成filter=True",
    "base_url": "http://192.168.33.5:9100",
    "variables": {
      "_getinfo": "${get_backend_info(YL)}",
      "connect_sid": "${get_connect_sid(YL)}",
      "headers_cookie": "${get_headers_cookie(YL)}"
    }
  },
  "teststeps": [
    {
      "name": "/sportDeliveryReport/create",
      "request": {
        "method": "GET",
        "url": "/sportDeliveryReport/create",
        "params": {
          "channelIds": "1",
          "createDate1": "2023-02-02",
          "createDate2": "2023-02-04"
        },
        "headers": {
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        }
      ]
    },
    {
      "name": "/sportDeliveryReport/InitData",
      "request": {
        "method": "GET",
        "url": "/sportDeliveryReport/InitData",
        "params": {
          "_": "1675676437222",
          "channelId": "",
          "limit": "10",
          "offset": "0",
          "statisDate": "2023-02"
        },
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest"
        },
        "cookies": {
          "connect.sid": "$connect_sid"
        }
      },
      "validate": [
        {
          "check": "status_code",
          "assert": "eq",
          "expect": 200
        },
        {
          "check": "headers.\"Content-Type\"",
          "assert": "eq",
          "expect": "text/plain;charset=utf-8"
        }
      ]
    }
  ]
}